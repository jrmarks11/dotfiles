#!/bin/sh

for branch in $(git for-each-ref --format='%(refname)' refs/heads/); do
  name="${branch##*/}"

  # dont delete master pls thx
  if [[ $name == *"master"* ]]; then
    continue
  fi

  echo "Checking $name"

  merged="$(git log --grep=$name | grep 'Merge pull request')"
  if [[ -n $merged ]]; then
    git branch -D $name
  fi
done
