# This is Git's per-user configuration file.
[user]
	name = John Marks
	email = jmarks@covermymeds.com
	username = jrmarks11
[core]
	excludesfile = /Users/jmarks/.gitignore_global
[push]
	default = current
[hub]
	host = git.innova-partners.com
[alias]
  bclean = "!git checkout master && git branch --merged | grep -v '\\*' | xargs -n 1 git branch -d"
  newb = "!f() { tNum=$1; bName=$2; git checkout -b \"${tNum}-${bName}\"; }; f"
  s = status -s
  up = !git pull --rebase --prune $@ && git submodule update --init --recursive
